{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "$API_BASE_URL",
        "EXPO_PUBLIC_KINOPOISK_API_KEY": "$KINOPOISK_API_KEY",
        "EXPO_PUBLIC_RAWG_API_KEY": "$RAWG_API_KEY",
        "EXPO_PUBLIC_YANDEX_JS_API_KEY": "$YANDEX_JS_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "$API_BASE_URL",
        "EXPO_PUBLIC_KINOPOISK_API_KEY": "$KINOPOISK_API_KEY",
        "EXPO_PUBLIC_RAWG_API_KEY": "$RAWG_API_KEY",
        "EXPO_PUBLIC_YANDEX_JS_API_KEY": "$YANDEX_JS_API_KEY"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "$API_BASE_URL",
        "EXPO_PUBLIC_KINOPOISK_API_KEY": "$KINOPOISK_API_KEY",
        "EXPO_PUBLIC_RAWG_API_KEY": "$RAWG_API_KEY",
        "EXPO_PUBLIC_YANDEX_JS_API_KEY": "$YANDEX_JS_API_KEY"
      }
    },
    "production-apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}