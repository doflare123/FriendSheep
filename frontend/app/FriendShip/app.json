{
  "expo": {
    "name": "FriendShip",
    "slug": "FriendShip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "friendship",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FriendShip"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.FriendShip"
    },
    "plugins": [
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns":[
      "**/*"
    ]
  }
}
